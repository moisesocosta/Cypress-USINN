  "scripts": {
    "cy:open": "cypress open",
    "cy:open:mobile": "cypress open --config viewportWidth=410 viewportHeight=860",
    "test": "cypress run",
    "test:mobile": "cypress run --config viewportWidth=410 viewportHeight=860",
    "test:local-api": "cypress run --spec 'cypress/e2e/local/api'",
    "test:local-funcional": "cypress run --spec 'cypress/e2e/local/funcional'",
    "test:produto-final-compatibilidade": "cypress run --spec 'cypress/e2e/produto-final/compatibilidade'",
    "test:produto-final-funcional": "cypress run --spec 'cypress/e2e/produto-final/funcional'",
    "test:produto-final-seguranca": "cypress run --spec 'cypress/e2e/produto-final/seguranca/'",
    "test:produto-final-usabilidade": "cypress run --spec 'cypress/e2e/produto-final/usabilidade/'",
    "test:webkit": "cypress run --browser webkit"
  },
