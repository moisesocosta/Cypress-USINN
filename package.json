{
  "name": "cypress-usinn",
  "version": "1.0.0",
  "description": "Projeto de testes automatizados para o sistema USINN MODELER",
  "main": "index.js",
  "scripts": {
    "start": "cypress open",
    "mobile": "cypress open --config viewportWidth=410 viewportHeight=860",
    "test": "cypress run",
    "test:mobile": "cypress run --config viewportWidth=410 viewportHeight=860",
    "test:api": "cypress run --spec 'cypress/e2e/tests/api'",
    "test:compatibilidade": "cypress run --spec 'cypress/e2e/tests/compatibilidade'",
    "test:funcional": "cypress run --spec 'cypress/e2e/tests/funcional'",
    "test:webkit": "cypress run --browser webkit"
  },
  "keywords": [],
  "author": "Mois√©s Costa <mosolicos@gmail.com>",
  "license": "ISC",
  "dependencies": {
    "cypress": "^13.16.0",
    "cypress-drag-drop": "^1.1.1",
    "cypress-plugin-api": "^2.11.1",
    "cypress-usinn": "file:"
  },
  "devDependencies": {
    "@4tw/cypress-drag-drop": "^2.2.5",
    "@faker-js/faker": "^8.4.1",
    "eslint": "^9.2.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-cypress": "^3.2.0",
    "playwright-webkit": "^1.44.0"
  }
}
